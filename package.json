{
  "name": "workflow-sandbox",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "clean:bin": "rimraf ./bin",
    "clean:dist": "rimraf ./dist",
    "clean": "run-p --silent clean:**",
    "copy": "copyfiles -u 2 \"src/assets/**/*.*\" dist",
    "watch:assets": "onchange -k \"src/assets/**/*.*\" -- npm run copy",
    "watch": "run-p --silent watch:**",
    "vite": "vite --port 8250",
    "build": "NODE_ENV=production vite build",
    "start": "npm-run-all -s clean copy -p watch vite"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@eslint/js": "^9.21.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@stylistic/eslint-plugin-jsx": "^4.2.0",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@types/node": "^25.2.2",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "copyfiles": "^2.4.1",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.2.0",
    "vite-plugin-eslint": "^1.8.1",
    "yarn": "^1.22.22"
  },
  "dependencies": {
    "@n3e/styled": "^1.0.2",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  }
}
